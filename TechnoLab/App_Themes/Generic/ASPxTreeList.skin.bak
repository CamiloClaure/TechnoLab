<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v17.1, Version=17.1.5.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register TagPrefix="dx" Namespace="DevExpress.Web.ASPxTreeList" Assembly="DevExpress.Web.ASPxTreeList.v17.1, Version=17.1.5.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>

<dx:ASPxTreeList runat="server" CssFilePath="{0}/styles.css" CssPostfix="Metropolis1">
    <Settings ShowTreeLines="False" />

    <SettingsPager CurrentPageNumberFormat="{0}">
        <PageSizeItemSettings ShowPopupShadow="False" />
    </SettingsPager>

    <Images SpriteCssFilePath="{0}/sprite.css" />

    <Styles CssPostfix="Metropolis1" CssFilePath="{0}/styles.css" />

    <StylesPager>
        <Pager ItemSpacing="1px" Spacing="1px" />
    </StylesPager>

    <StylesEditors>
        <Calendar>
            <Border BorderColor="Silver" BorderStyle="Solid" BorderWidth="1px" />
        </Calendar>
    </StylesEditors>

    <SettingsCustomizationWindow PopupWidth="220px" />

</dx:ASPxTreeList>

