                                </dx:MenuItem>
                             <%--   <dx:MenuItem Text="" Name="Back">
                                    <ItemStyle CssClass="toggle-item vertically-aligned" />
                                    <Image Url="../Img/Btn/back.svg" Height="18px" Width="18px" />
                                </dx:MenuItem>--%>
                              <%--  <dx:MenuItem Text="" ItemStyle-CssClass="image-item vertically-aligned" NavigateUrl="~/">
                                    <Image SpriteProperties-CssClass="header-logo" />
                                </dx:MenuItem>--%>
                            </Items>
                            <ClientSideEvents ItemClick="onLeftMenuItemClick" />
                        </dx:ASPxMenu>
                    </div>

                         <div class ="right" style ="margin-right :70px">

                            <img src="../Img/imgMasterPage/usuariocabecera.png"  style ="margin:8px 2px 0 0;" />
                            <dx:ASPxLabel ID="dlblNombreUsuario" runat="server" Text="LOREM IPSU" CssClass ="cWhite tSize-15 vaTop line-height-35" ></dx:ASPxLabel>
                            &nbsp;&nbsp;&nbsp;&nbsp;
      
                            <img src="../Img/imgMasterPage/calendarclock.png"  style ="margin:8px 2px 0 0;" width="24" height="24" />
                            <dx:ASPxLabel ID="dlblFecha" runat="server" Text="LOREM IPSU" CssClass ="cWhite tSize-15 vaTop line-height-35" ></dx:ASPxLabel>
                           <%--    &nbsp;&nbsp;&nbsp;&nbsp;
                               <img src="../Img/imgMasterPage/icono-herramienta001.png"  style ="margin:8px 2px 0 0;" width="24" height="24" />          
                                <dx:ASPxLabel ID="dlblCargo" runat="server" Text="LOREM IPSU" CssClass ="cWhite tSize-15 vaTop line-height-35" ></dx:ASPxLabel>--%>
                        </div>

               <%--   <dx:BootstrapMenu ID="BootstrapMenu1" runat="server">
                            <Items>
                                <dx:BootstrapMenuItem Text="Usuarios" >

                                </dx:BootstrapMenuItem>
                                <dx:BootstrapMenuItem Text="Ayudas" >

                                </dx:BootstrapMenuItem>
                            </Items>
                    </dx:BootstrapMenu>--%>
       
                    <div class="dx-clear"></div>
                </dx:PanelContent>
            </PanelCollection>
        </dx:ASPxPanel>

        <dx:ASPxPanel runat="server" ID="LeftPanel" ClientInstanceName="leftPanel" BackColor="#3C72AE"
            Collapsible="true"  FixedPosition="WindowLeft" Width="272px" 
            CssClass="left-panel" Paddings-Padding="0" Styles-ExpandBar-CssClass="expand-bar">
            <SettingsAdaptivity CollapseAtWindowInnerWidth="960" />
            <SettingsCollapsing ExpandButton-Visible="false" ExpandEffect="PopupToRight" AnimationType="Slide" />
            <PanelCollection>
                <dx:PanelContent>
                         <br />
                    <table class="tableCenter">
                                <tr>  
                                    <td>
                                        <dx:BootstrapButton ID="dbtnCerrarSesion" runat="server" Width="180px" Text="Cerrar Sesión"  
                                                                AutoPostBack="true"  OnClick="dbtnCerrarSesion_Click">

                                        </dx:BootstrapButton>
                                    </td>
                                </tr>
                            </table>
                    <br />
                  <dx:ASPxTreeView ID="dtvwMenuOpciones"  runat="server" EnableClientSideAPI="true"  EnableTheming="true" 
